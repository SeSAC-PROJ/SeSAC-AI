
{
	"name": "KSEB-AI Dev Container",
	"build": {
		"dockerfile": "../Dockerfile",
		"cacheFrom": "yuneu6112/kseb-ai:cache"
	},
	"forwardPorts": [8000],
	"postCreateCommand": "poetry install --no-root",
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python"
			]
		}
	},
	"runArgs": ["--name", "kseb-ai-dev-container"]
}
